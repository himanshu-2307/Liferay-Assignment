<?xml version="1.0"?>

<model-hints>
	<model name="com.liferay.assignment.office.model.Department">
		<field name="uuid" type="String" />
		<field name="departmentId" type="long" />
		<field name="groupId" type="long" />
		<field name="companyId" type="long" />
		<field name="userId" type="long" />
		<field name="userName" type="String" />
		<field name="createDate" type="Date" />
		<field name="modifiedDate" type="Date" />
		<field name="deptName" type="String" />
	</model>
	<model name="com.liferay.assignment.office.model.Employee">
		<field name="uuid" type="String" />
		<field name="employeeId" type="long" />
		<field name="groupId" type="long" />
		<field name="companyId" type="long" />
		<field name="userId" type="long" />
		<field name="userName" type="String" />
		<field name="createDate" type="Date" />
		<field name="modifiedDate" type="Date" />
		<field name="name" type="String" />
		<field name="jobTitle" type="String">
			<hint name="max-length">1024</hint>
		</field>
		<field name="phoneNo" type="long" />
		<field name="salary" type="long" />
		<field name="projectId" type="long" />
		<field name="departmentId" type="long" />
	</model>
	<model name="com.liferay.assignment.office.model.Project">
		<field name="uuid" type="String" />
		<field name="projectId" type="long" />
		<field name="groupId" type="long" />
		<field name="companyId" type="long" />
		<field name="userId" type="long" />
		<field name="userName" type="String" />
		<field name="createDate" type="Date" />
		<field name="modifiedDate" type="Date" />
		<field name="projectName" type="String" />
	</model>
</model-hints>